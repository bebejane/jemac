{
  "name": "jemac",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo & npm run codegen",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "clear": "rm -rf ./node_modules && rm -rf .next && pnpm i",
    "clear:cache": "rm -rf .next/cache",
    "codegen": "graphql-codegen --watch",
    "start": "next start",
    "lint": "next lint",
    "preview": "npm run build && npm run start",
    "reset": "rm -rf .next node_modules pnpm-lock.yaml ; pnpm i",
    "update-utils": "pnpm up github:bebejane/next-dato-util && git add . && git commit -m \"next-dato-utils up\" && git push",
    "generate-gql": "tsx generate-gql.ts"
  },
  "dependencies": {
    "@datocms/cma-client": "^5.1.26",
    "@datocms/cma-client-browser": "^5.1.26",
    "@datocms/cma-client-node": "^5.1.26",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/third-parties": "^15.3.4",
    "@u-wave/react-vimeo": "^0.9.12",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.5.0",
    "google": "link:@next/third-parties/google",
    "graphql": "^16.11.0",
    "motion": "^12.16.0",
    "next": "15.5.6",
    "next-dato-utils": "github:bebejane/next-dato-utils#latest",
    "next-intl": "^4.4.0",
    "react-datocms": "8.0.0-1",
    "react-use": "^17.6.0",
    "react-youtube": "^10.1.0",
    "rooks": "^7.14.1",
    "sass": "1.80.4",
    "swiper": "^11.2.8",
    "zod": "^3.25.32",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/typed-document-node": "^6.0.2",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-document-nodes": "^5.0.2",
    "@graphql-codegen/typescript-graphql-files-modules": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@parcel/watcher": "^2.5.1",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "graphql-config": "^5.1.5"
  }
}